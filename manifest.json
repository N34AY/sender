{
	"manifest_version": 2,
	"name": "Sender",
	"description": "Sender",
	"version": "1.0.0",
  "icons": {
    "128": "128.png",
    "64": "64.png",
    "32": "32.png"
  },
	"browser_action": {
		"default_icon": "64.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "css": [ "css/modal.css" ],
    "matches": ["https://find-bride.com/*"],
    "js": [
      "lib/axios.min.js",
      "js/main.js",
      "js/display.js",
      "js/logs.js",
      "js/blacklist.js",
      "js/notifications.js"
    ]
  }],
    "background": {
        "scripts": ["js/background.js"]
    },
  "permissions": ["activeTab", "notifications"]
}