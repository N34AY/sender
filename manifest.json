{
	"name": "Sender",
	"description": "Sender",
	"version": "1.0.0",
  "icons": {
    "128": "128.png",
    "64": "64.png",
    "32": "32.png"
  },
  "content_scripts": [{
    "css": [ "css/modal.css" ],
    "matches": ["https://find-bride.com/*"],
    "js": [
      "lib/axios.min.js",
      "lib/sweetalert2@10.js",
      "modules/auth/auth.js",
      "js/main.js",
      "modules/extension/extension.js",
      "modules/getMans/getMans.js",
      "modules/girlPhotos/girlPhotos.js",
      "modules/sendLetters/sendLetters.js",
      "modules/sendMessage/sendMessage.js",
      "modules/liker/liker.js",
      "js/logs.js",
      "js/blacklist.js",
      "js/notifications.js"
    ]
  }],
    "background": {
        "scripts": [
          "js/background.js"
        ]
    },
  "permissions": ["activeTab", "notifications"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "32": "32.png",
      "48": "64.png",
      "128": "128.png"
    }
  },
