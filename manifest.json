{
	"manifest_version": 2,
	"name": "Sender",
	"description": "Sender",
	"version": "1.0.0",
	"icons": {"128": "icon_128.png"},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
    },
    "content_scripts": [
        {
          "matches": ["https://find-bride.com/*"],
          "js": [
            "auth.js"
          ]
        }
      ],
    "background": {
        "scripts": ["background.js"]
    },
	"permissions": ["activeTab"]
}